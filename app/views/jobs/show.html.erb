<div class="container">
  <div class="col col-md-8">
    <p id="notice"><%= notice %></p>

    <p>
      <strong>Title:</strong>
      <%= @job.title %>
    </p>

    <p>
      <strong>Company:</strong>
      <%= @job.company %>
    </p>

    <p>
      <strong>Description:</strong>
      <%= @job.description %>
    </p>

    <p>
      <strong>Url:</strong>
      <%= @job.url %>
    </p>

    <p>
      <strong>Experience:</strong>
      <%= @job.category.name %>
    </p>

    <%= link_to 'Edit', edit_job_path(@job) %> |
    <%= link_to 'Back', jobs_path %>
  </div>
</div>
